#include <stdio.h>
#include <stdlib.h>

int giai_thua(int a)
{
    int i,c=1;
    for(i=1; i<=a; i++)
    {
        c=c*i;
    }
    return c;
}

int main()
{
    int x,c;
    printf("Nhap so :\n");
    scanf("%d",&x);
    printf("=======================|\n");
    c=giai_thua(x);
    printf("%d! = %d\n",x,c);
    return 0;
}
